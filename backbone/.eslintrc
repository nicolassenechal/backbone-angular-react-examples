{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "spyOn": true,
        "beforeEach": true,
        "afterEach": true,
        "describe": true,
        "fdescribe": true,
        "it": true,
        "fit": true,
        "expect": true,
        "jasmine": true
    },
    "rules": {
        "strict": 0,
        "quotes": [2, "single"],
        "indent": [2, 4],
        "max-depth": [2, 3],
        "max-statements": [2, 15],
        "complexity": [2, 6],
        "max-params": [2, 3],
        "no-bitwise": 2,
        "radix": 2,
        "max-nested-callbacks": [2, 4],
        "no-nested-ternary": 2,
        "space-after-keywords": [2, "always"],
        "space-before-blocks": [2, "always"],
        "space-before-function-paren": [2, "always"],
        "no-underscore-dangle": 0
    }
}
